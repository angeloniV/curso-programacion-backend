<p class="text-start mt-3  mb-3" style="font-weight: bold;">Registrar usuario</p>

<form action="/session/register" method="post" style="border:2px solid #0d6efd; border-radius: 5px;background-color:#0d6efd">
  <div class="row">
    <div class="col-md-4 m-2">
        <input type="text" class="form-control" id="formGroupExampleInput" placeholder="Nombre"  name="first_name">
    </div>
    <div class="col-md-4 m-2">
        <input type="text" class="form-control" id="formGroupExampleInput2" placeholder="Apellido"name="last_name">
    </div>
  </div>
  <div class="row">
    <div class="col-md-4 m-2">
        <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Email" name="email">
    </div>
    <div class="col-md-4 m-2">
        <input type="number" class="form-control" id="formGroupExampleInput2" placeholder="Edad" name="age">
    </div>
  </div>
  <div class="row">
    <div class="col-md-4 m-2">
        <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password" name="password">
    </div>
  </div>
  <div class="row">
    <div class="col-md-4 m-2">
        <button type="submit" class="btn btn-success">Registar</button>
    </div>
  </div>
</form>
